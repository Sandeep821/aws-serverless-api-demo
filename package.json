{
    "name": "vci-ofa-data-transfer",
    "version": "1.0.0",
    "description": "Serverless webpack example using Typescript",
    "main": "handler.ts",
    "scripts": {
        "test": "set NODE_ENV=unit-test&& jest tests/unittests/ --coverage --coverageDirectory ./coverage/unit-test  --passWithNoTests --testResultsProcessor ./node_modules/jest-sonar-reporter/index.js",
        "local": "SET DEBUG=* && serverless offline --env=dev -s dev && node --inspect"
    },
    "dependencies": {
        "bluebird": "^3.5.5",
        "jest-junit": "^6.3.0",
        "jest-sonar-reporter": "^2.0.0",
        "redis": "^2.8.0",
        "serverless": "^1.36.3",
        "sonarqube-scanner": "^2.4.0",
        "source-map-support": "^0.5.10",
        "tslint-sonarts": "^1.9.0"
    },
    "devDependencies": {
        "uuid": "^3.3.2",
        "@types/aws-lambda": "8.10.1",
        "@types/dotenv": "^6.1.0",
        "@types/jest": "^23.3.14",
        "@types/moxios": "^0.4.8",
        "@types/node": "8.0.57",
        "dotenv": "^6.2.0",
        "jest": "29.3.1",
        "moxios": "^0.4.0",
        "serverless-offline": "^4.2.1",
        "serverless-pseudo-parameters": "2.4.0",
        "serverless-webpack": "5.1.1",
        "source-map-support": "0.5.6",
        "ts-jest": "^23.10.5",
        "ts-loader": "9.4.2",
        "tslint": "^5.12.1",
        "tslint-config-airbnb": "^5.11.1",
        "tslint-config-prettier": "^1.17.0",
        "typescript": "^3.2.4",
        "webpack": "^5.75.0"
    },
    "jest": {
        "globals": {
            "ts-jest": {
                "diagnostics": false
            }
        },
        "roots": [
            "<rootDir>/tests"
        ],
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ],
        "testEnvironment": "node",
        "collectCoverage": true,
        "collectCoverageFrom": [
            "**/*.{ts,tsx}",
            "!**/index.{ts}",
            "!**/node_modules/**",
            "!**/dist/**",
            "!**/coverage/**",
            "!**/tests/**",
            "!**/webpack*",
            "!**/_*.{ts,tsx}"
        ],
        "reporters": [
            "jest-junit"
        ],
        "coverageReporters": [
            "lcov"
        ]
    },
    "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
    "license": "MIT"
}
